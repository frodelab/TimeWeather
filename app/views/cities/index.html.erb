<div class="col-sm-12">
  <h1>Cities#index</h1>
  <table class="table">
    <thead>
      <tr>
        <th>Ciudad</th>
        <th>Abreviatura</th>
        <th>Latitude</th>
        <th>Longitude</th>
      </tr>
    </thead>
    <tbody>
      <% @cities.each do |city| %>
        <tr>
          <td><%= city['name'] %></td>
          <td><%= city['short_name'] %></td>
          <td><%= city['latitude'] %></td>
          <td><%= city['longitude'] %></td>
        </tr>
      <% end %>
    </tbody>
</div>
